<dashboard script="demoviz_page2.js">
    <label>Multigraph Dashboard</label>

    <row>
        <html>
            <div id="hook"></div>
        </html>
    </row>
    <row>
        <panel>
          <chart>
            <title>Details</title>
            <search>
              <query>index=cityweather | search $filter$ | timechart span=1w avg("Mean TemperatureF") by source</query>
              <earliest>0</earliest>
              <latest></latest>
            </search>
            <option name="charting.chart">line</option>
          </chart>
        </panel>
    </row>

</dashboard>